// POST http://localhost:5555/conversion/route
{
  "spark" : {
    "app-name": "handler",
    "master-url": "local[*]",
    "log-level" : {
      "type": "warn"
    }
  },
  "route": {
    "type": "json-to-kafka",
    "in": "src/test/resources/json_to_kafka-data/input",
    "out": "spark-plugin-topic-2",
    "broker-urls": "localhost:9092",
    "kafka-mode": {
      "type": "spark-kafka-producer-plugin",
      "use-stream": false
    }
  }
}