// POST http://localhost:5555/conversion/route
{
  "route": {
    "type": "elasticsearch-to-file",
    "in": "elasticsearch-index",
    "out": "your-output-folder-containing-json-files",
    "file-system": {
      "type": "hdfs or local"
    },
    "search-query": {
      "type": "term-query",
      "field": {
        "name": "field_name",
        "value": "field_value"
      }
    }
  }
}