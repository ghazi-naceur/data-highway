// POST http://localhost:5555/conversion/route
{
  "route": {
    "type": "elasticsearch-to-file",
    "in": "elasticsearch-index",
    "out": "your-output-folder-containing-json-files",
    "file-system": {
      "type": "hdfs or local"
    },
    "search-query": {
      "type": "range-query",
      "range-field": {
        "range-type": {
          "type": "range-type"
        },
        "name": "field-name",
        "lte": "lower than or equal value",
        "gte": "greater than or equal value"
      }
    }
  }
}